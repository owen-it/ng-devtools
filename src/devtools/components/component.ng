<template>
    <div></div>
</template>

<script>
    export default {
        bindings: {
            is: '='
        },
        controller: ['$scope', '$element', '$compile', function($scope, $element, $compile){
            $scope.$watch('$ctrl.is', component => {
                $element.html(`<${component}></${component}>`)
                $compile($element.contents())($scope)
            })
        }]
    }
</script>